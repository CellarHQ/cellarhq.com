{{#partial "header"}}
  <h1>Oops! <small>Error ID: {{correlationId}}</small></h1>
{{/partial}}
{{# partial "content" }}
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1 col-md-10 col-md-offset-1 col-lg-10 col-lg-offset-1">
